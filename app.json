{
  "expo": {
    "name": "good-steward",
    "slug": "good-steward",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/good-steward-logo.png",
    "scheme": "goodsteward",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.good-steward",
      "deploymentTarget": "15.5"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#4A9FB5",
        "foregroundImage": "./assets/images/good-steward-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jaehyukchoi.goodsteward",
      "versionCode": 1,
      "minSdkVersion": 26,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/good-steward-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/good-steward-logo.png",
            "backgroundColor": "#1a1a1a"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Good Steward needs camera access to scan barcodes and identify food."
        }
      ],
      "react-native-health-connect",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.5"
          }
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5e675e5c-f664-4c5b-984c-e71c006737ef"
      }
    }
  }
}
